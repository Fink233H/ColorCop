IDR_COPMENU MENU
BEGIN
    POPUP "POPUP"
    BEGIN
        POPUP "Color"
        BEGIN
            MENUITEM "&Snap to WebSafe\tCtrl+W",    ID_COLOR_SNAPTOWEBSAFE
            MENUITEM "Detect Websafe",              ID_POPUP_COLOR_DETECTWEBSAFE
            MENUITEM SEPARATOR
            MENUITEM "&Reverse\tCtrl+R",            ID_COLOR_REVERSE
            MENUITEM "Ra&ndom\tCtrl+Z",             ID_COLOR_RANDOM
            MENUITEM "Convert to Grayscale\tCtrl+G", ID_POPUP_COLOR_CONVERTTOGRAYSCALE
        END
        POPUP "Mode"
        BEGIN
            MENUITEM "&HTML Hex\tCtrl+H",           ID_VIEW_HTMLHEXMODE, CHECKED
            MENUITEM "&Delphi Hex\tCtrl+D",         ID_OPTIONS_DELPHIMODE
            MENUITEM "PowerBuilder\tCtrl+P",        ID_POPUP_HEXMODE_POWERBUILDER
            MENUITEM "Visual Basic Hex\tCtrl+B",    ID_POPUP_MODE_VISUALBASICHEX
            MENUITEM "Visual C++ Hex\tCtrl+Y",      ID_POPUP_MODE_VISUALCHEX
            MENUITEM "RGB float",                   ID_POPUP_MODE_RGBFLOAT
            MENUITEM "RGB int",                     ID_POPUP_MODE_RGBINT
            MENUITEM "Clarion Hex",                 ID_POPUP_MODE_CLARIONHEX
        END
        POPUP "Space"
        BEGIN
            MENUITEM "RGB",                         ID_POPUP_SPACE_RGB
            MENUITEM "CMYK",                        ID_POPUP_SPACE_CMYK
        END
        POPUP "Sampling"
        BEGIN
            MENUITEM "&1 Pixel\tCtrl+1",            ID_POPUP_SAMPLING_1PIXEL, CHECKED
            MENUITEM "&3 by 3 Average\tCtrl+3",     ID_POPUP_SAMPLING_3BY3AVERAGE
            MENUITEM "&5 by 5 Average\tCtrl+5",     ID_POPUP_SAMPLING_5BY5AVERAGE
            MENUITEM "Multi-Pixel Average",         ID_POPUP_SAMPLING_MULTIPIXEL
            MENUITEM SEPARATOR
            MENUITEM "Increase Multi-Pixel Average\t(Plus) +", ID_POPUP_SAMPLING_INCREASEMULTIPIXELAVERAGE
            MENUITEM "Decrease Multi-Pixel Average\t(Minus) -", ID_POPUP_SAMPLING_DECREASEMULTIPIXELAVERAGE
        END
        POPUP "Options"
        BEGIN
            MENUITEM "&Auto Copy Hex to Clipboard\tCtrl+K", ID_OPTIONS_AUTOCOPYTOCLIPBOARD, CHECKED
            MENUITEM "&Omit Hex Symbol\tCtrl+O",    ID_OPTIONS_OMITSYMBOL
            MENUITEM "&Uppercase Hex\tCtrl+U",      ID_OPTIONS_UPPERCASEHEX, CHECKED
            MENUITEM "Magnify while Eyedropping",   ID_POPUP_OPTIONS_MAGNIFYWHILEEYEDROPPING, CHECKED
            MENUITEM "Use Cross Hair Cursor",       ID_POPUP_OPTIONS_USECROSSHAIRCURSOR
            MENUITEM "Put cursor over Eyedropper on Startup", ID_POPUP_OPTIONS_STARTCURSORONEYEDROPPER
        END
        POPUP "Application"
        BEGIN
            MENUITEM "&Always on &Top\tCtrl+T",     ID_OPTIONS_ALWAYSONTOP, CHECKED
            MENUITEM "EasyMo&ve\tCtrl+N",           ID_POPUP_APPLICATION_EASYMOVE
            MENUITEM "&Expanded Dialog\tCtrl+E",    ID_POPUP_APPLICATION_EXPANDEDDIALOG
            MENUITEM "&Minimize to System tray\tCtrl+M", ID_OPTIONS_MINIMIZETOSYSTRAY
            MENUITEM "Minimize on Startup",         ID_POPUP_APPLICATION_MINIMIZETOSYSTEMTRAYONSTART
            MENUITEM "Allow Multiple Instances",    ID_POPUP_APPLICATION_ALLOWMULTIPLEINSTANCES, CHECKED
            MENUITEM SEPARATOR
            MENUITEM "About &Color Cop",            ID_FILE_ABOUT
            MENUITEM "&Help\tF1",                   ID_POPUP_APPLICATION_HELP
        END
        MENUITEM "Exit",                        ID_POPUP_EXIT
    END
END

IDR_SYSTRAY_MENU MENU
BEGIN
    POPUP "POPUP"
    BEGIN
        MENUITEM "Restore",                     ID_POPUP_RESTORE
        MENUITEM "Exit",                        ID_POPUP_EXIT
    END
END


